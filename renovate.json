{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "configMigration": true,
  "packageRules": [
    {
      "groupName": "vercel monorepo",
      "matchSourceUrls": ["https://github.com/vercel/vercel"]
    },
    {
      "groupName": "sentry monorepo",
      "matchPackagePrefixes": ["@sentry/"]
    }
  ]
}
